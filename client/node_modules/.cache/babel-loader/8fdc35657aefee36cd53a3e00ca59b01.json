 = true\n}\n  },\n  mounted() {\n    this.closestNetworks();\n  }\n};\n</script>\n\n<style scoped>\ntable {\n  border: 1px solid rgb(85, 85, 85);\n  background-color: rgba(65, 64, 64, .5);\n  border-radius: 10px;\n  width: 100%;\n  color: white;\n}\ntr > th {\n  color: white;\n  width: 25%;\n}\n.loader-position {\n  margin: 0 auto;\n}\n.network-row:hover {\n  background-color: rgba(47, 75, 109, 0.5);\n  cursor: pointer;\n}\n</style>\n"],"sourceRoot":"src/components"}]}